{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Result",
    "type": "object",
    "properties": {
        "datasources": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "results": {
            "type": "array",
            "items": {
                "anyOf": [
                    {
                        "$ref": "file:src/schemas/person.json"
                    },
                    {
                        "$ref": "file:src/schemas/article.json"
                    }
                ]
            }
        }
    },
    "required": [
        "datasources",
        "results"
    ]
}