{
  "name": "apiadapter",
  "version": "1.0.0",
  "description": "API Adapter",
  "main": "app.js",
  "scripts": {
    "pretest": "eslint --ignore-path .gitignore .",
    "start": "export NODE_ENV=development && node index.js",
    "test": "export NODE_ENV=test INTEGRATION_TESTS=false && ./node_modules/.bin/mocha --require babel-core/register src/**/*.test.js src/**/**/*.test.js src/**/**/**/*.test.js --reporter spec",
    "integration-test": "export NODE_ENV=test INTEGRATION_TESTS=true &&  ./node_modules/.bin/mocha --require babel-core/register src/**/*.test.js src/**/**/*.test.js src/**/**/**/*.test.js --reporter spec",
    "migrate": "./node_modules/.bin/migrate"
  },
  "keywords": [
    "api",
    "adapter"
  ],
  "author": "Toni Iltanen",
  "license": "ISC",
  "dependencies": {
    "2": "^1.0.2",
    "amqplib": "^0.5.2",
    "bluebird": "^3.5.1",
    "config": "^1.30.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "jsonschema": "^1.2.4",
    "migrate-mongoose": "^3.2.2",
    "mongoose": "^5.1.5",
    "morgan": "^1.9.0",
    "restify": "^7.2.1",
    "restify-enroute": "^6.1.0",
    "restify-errors": "^6.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "spec": "^1.0.1"
  }
}
